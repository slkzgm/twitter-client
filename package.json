{
  "name": "twitter-client",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@sinclair/typebox": "^0.34.33",
    "glob": "11.0.2",
    "headers-polyfill": "^4.0.3",
    "json-stable-stringify": "^1.3.0",
    "otpauth": "^9.4.0",
    "set-cookie-parser": "^2.7.1",
    "tough-cookie": "^5.1.2",
    "twitter-api-v2": "^1.23.2",
    "typescript": "^5.2.2",
    "undici": "^7.10.0",
    "undici-types": "^7.10.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.1.4",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "prettier --write ./src",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist node_modules .turbo .turbo-tsconfig.json tsconfig.tsbuildinfo"
  }
}
